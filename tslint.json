{
  "extends": "tslint:recommended",
  "defaultSeverity": "error",
  "rules": {
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "log",
        "info",
        "warn",
        "error",
        "time",
        "trace",
        "timeEnd"
      ]
    }
  },
  "jsRules": {}
}
